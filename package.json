{
  "name": "sneakx",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "sync:test": "npx tsx scripts/test-sync.ts",
    "sync:initial": "npx tsx -e \"import { syncOrchestrator } from './src/lib/sync'; syncOrchestrator.performInitialSync().then(console.log).catch(console.error)\"",
    "sync:run": "npx tsx -e \"import { syncOrchestrator } from './src/lib/sync'; syncOrchestrator.runScheduledSync().then(console.log).catch(console.error)\"",
    "sync:health": "npx tsx -e \"import { syncOrchestrator } from './src/lib/sync'; syncOrchestrator.performHealthCheck().then(console.log).catch(console.error)\"",
    "sync:dashboard": "npx tsx -e \"import { syncOrchestrator } from './src/lib/sync'; syncOrchestrator.getDashboardData().then(console.log).catch(console.error)\"",
    "monitor:start": "npx tsx scripts/start-monitoring.ts",
    "monitor:health": "npx tsx -e \"import { getMonitorOrchestrator } from './src/lib/monitor/orchestrator'; getMonitorOrchestrator().performHealthCheck().then(console.log).catch(console.error)\"",
    "monitor:status": "npx tsx -e \"import { getMonitorOrchestrator } from './src/lib/monitor/orchestrator'; getMonitorOrchestrator().getSystemStatus().then(console.log).catch(console.error)\"",
    "monitor:stats": "npx tsx -e \"import { getMonitorOrchestrator } from './src/lib/monitor/orchestrator'; getMonitorOrchestrator().getMonitoringStats().then(console.log).catch(console.error)\"",
    "monitor:process-queue": "npx tsx -e \"import { getMonitorOrchestrator } from './src/lib/monitor/orchestrator'; getMonitorOrchestrator().processWebhookQueue().then(console.log).catch(console.error)\"",
    "monitor:maintenance": "npx tsx -e \"import { getMonitorOrchestrator } from './src/lib/monitor/orchestrator'; getMonitorOrchestrator().performMaintenance().then(console.log).catch(console.error)\""
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@react-email/components": "^0.5.3",
    "@react-email/html": "^0.0.11",
    "@react-email/render": "^1.2.3",
    "@react-email/text": "^0.1.5",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.90.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.292.0",
    "next": "^14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^4.2.11",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "resend": "^6.1.0",
    "sharp": "^0.32.6",
    "tailwind-merge": "2.6.0",
    "zod": "^3.25.76",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
